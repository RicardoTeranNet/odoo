<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="block_production_lot_wzd_form" model="ir.ui.view">
            <field name="name">block.production.lot.wzd.form</field>
            <field name="model">block.production.lot</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Lock Production Lot">
                    <separator colspan="4" string="Block specific production lot" />
                    <field name="case_name" colspan="4"/>
                    <newline/>
                    <field name="prodlot_id" colspan="2"/>
                    <field name="firmness_grade" colspan="2"/>
                    <field name="case_description" colspan="4"/>
                    <button special="cancel" string="Cancel" icon="gtk-cancel"/>
                    <button name="lock_production_lot" type="object" icon="gtk-execute" string="Block"/>
                </form>
            </field>
        </record>

    </data>
</openerp>